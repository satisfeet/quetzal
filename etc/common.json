{

  "name": "manager",

  "views": {
    "path": "/usr/views",
    "default": "jade"
  },

  "static": {
    "path": "/srv"
  },

  "styles": {
    "src": "/usr/styles",
    "dest": "/srv/stylesheets",
    "prefix": "stylesheets"
  },

  "builder": {
    "entry": "/",
    "output": {
      "script": "/srv/javascripts/build.js",
      "style": "/srv/stylesheets/build.css"
    },
    "install": true
  },

  "server": {
    "cert": {
      "path": "/etc/cert/cert.pfx"
    }
  }

}
